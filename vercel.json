{
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/[...path].js"
        },
        {
            "source": "/uploads/(.*)",
            "destination": "/wecare-backend/uploads/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}